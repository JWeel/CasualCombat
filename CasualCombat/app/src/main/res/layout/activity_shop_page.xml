<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="4dip"
    tools:context=".ShopPage"
    android:orientation="vertical"
    android:background="#EEEEEE"
    >

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_vertical"
        >
        <RelativeLayout
            android:layout_width="380dip"
            android:layout_height="200dip"
            android:layout_centerInParent="true"
            android:padding="4dip"
            android:background="#557766"
            android:id="@+id/shopLayoutCharacter"
            >
            <ImageView
                android:layout_width="64dip"
                android:layout_height="64dip"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_marginTop="4dip"
                android:background="@drawable/avatar"
                android:onClick="secretClick"
                android:id="@+id/shopCharIcon"
                />
            <TextView
                android:layout_width="80dip"
                android:layout_height="20dip"
                android:text="LEVEL 0"
                android:textColor="#FFFFFF"
                android:textSize="12sp"
                android:typeface="monospace"
                android:gravity="center_vertical"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_below="@id/shopCharIcon"
                android:layout_marginTop="8dip"
                android:layout_marginLeft="2dip"
                android:layout_marginStart="2dip"
                android:id="@+id/shopCharLevel"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="20dip"
                android:text="Points left to spend: 10"
                android:textColor="#FFFFFF"
                android:textSize="12sp"
                android:typeface="monospace"
                android:gravity="center_vertical"
                android:layout_toRightOf="@id/shopCharIcon"
                android:layout_toEndOf="@id/shopCharIcon"
                android:layout_below="@id/shopCharIcon"
                android:layout_marginTop="8dip"
                android:layout_marginLeft="20dip"
                android:layout_marginStart="20dip"
                android:id="@+id/shopCharPoints"
                />

            <TextView
                android:layout_width="90dip"
                android:layout_height="24dip"
                android:textColor="#FFFFFF"
                android:text=""
                android:textSize="14sp"
                android:textStyle="bold"
                android:typeface="monospace"
                android:gravity="center"
                android:layout_toRightOf="@id/shopCharIcon"
                android:layout_toEndOf="@id/shopCharIcon"
                android:layout_alignParentTop="true"
                android:layout_marginTop="6dip"
                android:layout_marginLeft="6dip"
                android:layout_marginStart="6dip"
                android:id="@+id/shopCharName"
                />

            <Button
                android:layout_width="30dip"
                android:layout_height="30dip"
                android:text="+"
                android:textSize="20sp"
                android:typeface="monospace"
                android:gravity="center"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentTop="true"
                android:layout_marginTop="4dip"
                android:layout_marginRight="4dip"
                android:layout_marginEnd="4dip"
                android:textColor="#FFFFFF"
                android:background="#11A0D0"
                android:onClick="addClick"
                android:id="@+id/shopCharHealthAdd"
                />
            <ProgressBar
                style="@android:style/Widget.ProgressBar.Horizontal"
                android:progressDrawable="@drawable/health_bar"
                android:layout_width="80dip"
                android:layout_height="20dip"
                android:layout_alignTop="@id/shopCharHealthAdd"
                android:layout_toLeftOf="@id/shopCharHealthAdd"
                android:layout_toStartOf="@id/shopCharHealthAdd"
                android:layout_marginTop="5dip"
                android:layout_marginRight="8dip"
                android:layout_marginEnd="8dip"
                android:id="@+id/shopCharHealth"
                />
            <TextView
                android:layout_width="50dip"
                android:layout_height="24dip"
                android:text="99/99"
                android:textColor="#FFFFFF"
                android:textSize="12sp"
                android:typeface="monospace"
                android:gravity="center"
                android:layout_alignTop="@id/shopCharHealthAdd"
                android:layout_alignLeft="@id/shopCharHealth"
                android:layout_alignStart="@id/shopCharHealth"
                android:layout_marginTop="3dip"
                android:layout_marginLeft="15dip"
                android:layout_marginStart="15dip"
                android:id="@+id/shopCharHealthText"
                />

            <Button
                android:layout_width="30dip"
                android:layout_height="30dip"
                android:text="+"
                android:textSize="20sp"
                android:typeface="monospace"
                android:gravity="center"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true"
                android:layout_below="@id/shopCharHealthAdd"
                android:layout_marginTop="4dip"
                android:layout_marginRight="4dip"
                android:layout_marginEnd="4dip"
                android:textColor="#FFFFFF"
                android:background="#11A0D0"
                android:onClick="addClick"
                android:id="@+id/shopCharMagicAdd"
                />
            <ProgressBar
                style="@android:style/Widget.ProgressBar.Horizontal"
                android:progressDrawable="@drawable/magic_bar"
                android:layout_width="80dip"
                android:layout_height="20dip"
                android:layout_alignTop="@id/shopCharMagicAdd"
                android:layout_toLeftOf="@id/shopCharMagicAdd"
                android:layout_toStartOf="@id/shopCharMagicAdd"
                android:layout_marginTop="5dip"
                android:layout_marginRight="8dip"
                android:layout_marginEnd="8dip"
                android:id="@+id/shopCharMagic"
                />
            <TextView
                android:layout_width="50dip"
                android:layout_height="24dip"
                android:text="99/99"
                android:textColor="#FFFFFF"
                android:textSize="12sp"
                android:typeface="monospace"
                android:gravity="center"
                android:layout_alignTop="@id/shopCharMagicAdd"
                android:layout_alignLeft="@id/shopCharMagic"
                android:layout_alignStart="@id/shopCharMagic"
                android:layout_marginTop="3dip"
                android:layout_marginLeft="15dip"
                android:layout_marginStart="15dip"
                android:id="@+id/shopCharMagicText"
                />

            <Button
                android:layout_width="30dip"
                android:layout_height="30dip"
                android:text="+"
                android:textSize="20sp"
                android:typeface="monospace"
                android:gravity="center"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_alignParentBottom="true"
                android:layout_marginLeft="10dip"
                android:layout_marginStart="10dip"
                android:layout_marginRight="20dip"
                android:layout_marginEnd="20dip"
                android:textColor="#FFFFFF"
                android:background="#11A0D0"
                android:onClick="addClick"
                android:id="@+id/shopCharStrengthAdd"
                />
            <Button
                android:layout_width="30dip"
                android:layout_height="30dip"
                android:text="+"
                android:textSize="20sp"
                android:typeface="monospace"
                android:gravity="center"
                android:layout_toRightOf="@id/shopCharStrengthAdd"
                android:layout_toEndOf="@id/shopCharStrengthAdd"
                android:layout_alignParentBottom="true"
                android:layout_marginLeft="20dip"
                android:layout_marginStart="20dip"
                android:layout_marginRight="20dip"
                android:layout_marginEnd="20dip"
                android:textColor="#FFFFFF"
                android:background="#11A0D0"
                android:onClick="addClick"
                android:id="@+id/shopCharWillpowerAdd"
                />
            <Button
                android:layout_width="30dip"
                android:layout_height="30dip"
                android:text="+"
                android:textSize="20sp"
                android:typeface="monospace"
                android:gravity="center"
                android:layout_toRightOf="@id/shopCharWillpowerAdd"
                android:layout_toEndOf="@id/shopCharWillpowerAdd"
                android:layout_alignParentBottom="true"
                android:layout_marginLeft="20dip"
                android:layout_marginStart="20dip"
                android:layout_marginRight="20dip"
                android:layout_marginEnd="20dip"
                android:textColor="#FFFFFF"
                android:background="#11A0D0"
                android:onClick="addClick"
                android:id="@+id/shopCharDefenseAdd"
                />
            <Button
                android:layout_width="30dip"
                android:layout_height="30dip"
                android:text="+"
                android:textSize="20sp"
                android:typeface="monospace"
                android:gravity="center"
                android:layout_toRightOf="@id/shopCharDefenseAdd"
                android:layout_toEndOf="@id/shopCharDefenseAdd"
                android:layout_alignParentBottom="true"
                android:layout_marginLeft="20dip"
                android:layout_marginStart="20dip"
                android:layout_marginRight="20dip"
                android:layout_marginEnd="20dip"
                android:textColor="#FFFFFF"
                android:background="#11A0D0"
                android:onClick="addClick"
                android:id="@+id/shopCharResistanceAdd"
                />
            <Button
                android:layout_width="30dip"
                android:layout_height="30dip"
                android:text="+"
                android:textSize="20sp"
                android:typeface="monospace"
                android:gravity="center"
                android:layout_toRightOf="@id/shopCharResistanceAdd"
                android:layout_toEndOf="@id/shopCharResistanceAdd"
                android:layout_alignParentBottom="true"
                android:layout_marginLeft="20dip"
                android:layout_marginStart="20dip"
                android:layout_marginRight="10dip"
                android:layout_marginEnd="10dip"
                android:textColor="#FFFFFF"
                android:background="#11A0D0"
                android:onClick="addClick"
                android:id="@+id/shopCharSpeedAdd"
                />

            <TextView
                android:layout_width="70dip"
                android:layout_height="20dip"
                android:text="STR 123"
                android:textColor="#FFFFFF"
                android:textSize="14sp"
                android:typeface="monospace"
                android:gravity="center"
                android:layout_above="@id/shopCharStrengthAdd"
                android:layout_alignLeft="@id/shopCharStrengthAdd"
                android:layout_alignStart="@id/shopCharStrengthAdd"
                android:layout_marginLeft="-13dip"
                android:layout_marginStart="-13dip"
                android:layout_marginBottom="4dip"
                android:id="@+id/shopCharStrength"
                />
            <TextView
                android:layout_width="70dip"
                android:layout_height="20dip"
                android:text="WIL 123"
                android:textColor="#FFFFFF"
                android:textSize="14sp"
                android:typeface="monospace"
                android:gravity="center"
                android:layout_above="@id/shopCharWillpowerAdd"
                android:layout_alignLeft="@id/shopCharWillpowerAdd"
                android:layout_alignStart="@id/shopCharWillpowerAdd"
                android:layout_marginLeft="-20dip"
                android:layout_marginStart="-20dip"
                android:layout_marginBottom="4dip"
                android:id="@+id/shopCharWillpower"
                />
            <TextView
                android:layout_width="70dip"
                android:layout_height="20dip"
                android:text="DEF 123"
                android:textColor="#FFFFFF"
                android:textSize="14sp"
                android:typeface="monospace"
                android:gravity="center"
                android:layout_above="@id/shopCharDefenseAdd"
                android:layout_alignLeft="@id/shopCharDefenseAdd"
                android:layout_alignStart="@id/shopCharDefenseAdd"
                android:layout_marginLeft="-20dip"
                android:layout_marginStart="-20dip"
                android:layout_marginBottom="4dip"
                android:id="@+id/shopCharDefense"
                />
            <TextView
                android:layout_width="70dip"
                android:layout_height="20dip"
                android:text="RES 123"
                android:textColor="#FFFFFF"
                android:textSize="14sp"
                android:typeface="monospace"
                android:gravity="center"
                android:layout_above="@id/shopCharResistanceAdd"
                android:layout_alignLeft="@id/shopCharResistanceAdd"
                android:layout_alignStart="@id/shopCharResistanceAdd"
                android:layout_marginLeft="-20dip"
                android:layout_marginStart="-20dip"
                android:layout_marginBottom="4dip"
                android:id="@+id/shopCharResistance"
                />
            <TextView
                android:layout_width="70dip"
                android:layout_height="20dip"
                android:text="SPD 123"
                android:textColor="#FFFFFF"
                android:textSize="14sp"
                android:typeface="monospace"
                android:gravity="center"
                android:layout_above="@id/shopCharSpeedAdd"
                android:layout_alignLeft="@id/shopCharSpeedAdd"
                android:layout_alignStart="@id/shopCharSpeedAdd"
                android:layout_marginLeft="-20dip"
                android:layout_marginStart="-20dip"
                android:layout_marginBottom="4dip"
                android:id="@+id/shopCharSpeed"
                />
        </RelativeLayout>
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="50dip"
        android:background="#557766"
        android:paddingBottom="4dip"
        android:id="@+id/shopLayoutEquipment"
        >
        <TextView
            android:layout_width="110dip"
            android:layout_height="36dip"
            android:text="RED NECKLACE\n+77RES"
            android:textColor="#FFFFFF"
            android:typeface="monospace"
            android:gravity="center"
            android:layout_centerHorizontal="true"
            android:layout_alignParentBottom="true"
            android:background="#779988"
            android:id="@+id/shopCharArmor"
            />
        <TextView
            android:layout_width="110dip"
            android:layout_height="36dip"
            android:text="WOODEN SWORD\n+50STR"
            android:textColor="#FFFFFF"
            android:typeface="monospace"
            android:gravity="center"
            android:layout_alignTop="@id/shopCharArmor"
            android:layout_toLeftOf="@id/shopCharArmor"
            android:layout_toStartOf="@id/shopCharArmor"
            android:layout_marginRight="8dip"
            android:layout_marginEnd="8dip"
            android:background="#779988"
            android:id="@+id/shopCharWeapon"
            />
        <TextView
            android:layout_width="110dip"
            android:layout_height="36dip"
            android:text="NICE SANDALS\n+66SPD"
            android:textColor="#FFFFFF"
            android:typeface="monospace"
            android:gravity="center"
            android:layout_alignTop="@id/shopCharArmor"
            android:layout_toRightOf="@id/shopCharArmor"
            android:layout_toEndOf="@id/shopCharArmor"
            android:layout_marginLeft="8dip"
            android:layout_marginStart="8dip"
            android:background="#779988"
            android:id="@+id/shopCharBoots"
            />
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="55dip"
        android:padding="6dip"
        android:id="@+id/shopLayoutMiddle"
        >
        <Button
            android:text="Save Progress"
            android:layout_width="180dip"
            android:layout_height="50dip"
            android:textColor="#459785"
            android:textSize="20sp"
            android:gravity="center"
            android:layout_alignParentBottom="true"
            android:onClick="saveClick"
            android:enabled="false"
            android:id="@+id/saveButton"
            />
        <Button
            android:text="Next Battle"
            android:layout_width="180dip"
            android:layout_height="50dip"
            android:textColor="#459785"
            android:textSize="20sp"
            android:gravity="center"
            android:layout_alignParentBottom="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:onClick="nextClick"
            android:enabled="false"
            android:id="@+id/nextButton"
            />
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:padding="8dip"
        android:background="#888888"
        >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="You have died, and so your adventure ends.\nTouch this message to return to the title screen, where you can reload your character and try again, or start a new adventure with a new character."
            android:textSize="20sp"
            android:textColor="#FFFFFF"
            android:gravity="center_horizontal"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:onClick="titleClick"
            android:clickable="true"
            android:visibility="invisible"
            android:id="@+id/shopDeathText"
            />
        <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:id="@+id/shopLayoutShop"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="SHOP"
                android:textColor="#FFFFFF"
                android:textSize="24sp"
                android:textStyle="bold"
                android:id="@+id/shopTitle"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="20dip"
                android:text="Current gold: 500"
                android:textColor="#FFFFFF"
                android:typeface="monospace"
                android:layout_below="@id/shopTitle"
                android:id="@+id/shopCharMoney"
                />
            <Button
                android:layout_width="150dp"
                android:layout_height="36dip"
                android:text="RESTORE HEALTH: 1G"
                android:textColor="#FF5555"
                android:textSize="12sp"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true"
                android:onClick="restoreHealthClick"
                android:id="@+id/shopHealthRegen"
                />
            <Button
                android:layout_width="150dp"
                android:layout_height="36dip"
                android:text="RESTORE MAGIC: 1G"
                android:textColor="#5555FF"
                android:textSize="12sp"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true"
                android:layout_below="@+id/shopHealthRegen"
                android:onClick="restoreMagicClick"
                android:id="@+id/shopMagicRegen"
                />
            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="100dip"
                android:layout_alignParentBottom="true"
                android:weightSum="3"
                >
                <RelativeLayout
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:background="#AAAAAA"
                    android:layout_weight="1"
                    android:layout_marginRight="2dip"
                    android:layout_marginEnd="2dip"
                    android:layout_marginLeft="2dip"
                    android:layout_marginStart="2dip"
                    android:id="@+id/shopLayoutUsable"
                    >
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="SOLD OUT"
                        android:textSize="16sp"
                        android:textStyle="bold"
                        android:textColor="#FFFFFF"
                        android:gravity="center"
                        android:layout_centerInParent="true"
                        android:id="@+id/shopWarningUsable"
                        />
                    <RelativeLayout
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:clickable="true"
                        android:onClick="usableClick"
                        android:id="@+id/shopLayoutUsableInner"
                        >
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="20G"
                            android:textColor="#FFFFFF"
                            android:layout_centerHorizontal="true"
                            android:layout_marginTop="10dip"
                            android:clickable="true"
                            android:onClick="usableClick"
                            android:id="@+id/shopPriceUsable"
                            />
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="20dip"
                            android:text="123456789012"
                            android:textColor="#FFFFFF"
                            android:textSize="14sp"
                            android:textStyle="bold"
                            android:maxLength="12"
                            android:maxLines="1"
                            android:typeface="monospace"
                            android:gravity="center"
                            android:layout_centerHorizontal="true"
                            android:layout_below="@id/shopPriceUsable"
                            android:layout_marginTop="2dip"
                            android:clickable="true"
                            android:onClick="usableClick"
                            android:id="@+id/shopNameUsable"
                            />
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="12345678901234567890121234567890123456789012"
                            android:textColor="#FFFFFF"
                            android:textSize="12sp"
                            android:typeface="monospace"
                            android:gravity="center"
                            android:layout_centerHorizontal="true"
                            android:layout_below="@+id/shopNameUsable"
                            android:layout_marginTop="2dip"
                            android:clickable="true"
                            android:onClick="usableClick"
                            android:id="@+id/shopInfoUsable"
                            />

                    </RelativeLayout>
                </RelativeLayout>
                <RelativeLayout
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:background="#AAAAAA"
                    android:layout_weight="1"
                    android:layout_marginRight="2dip"
                    android:layout_marginEnd="2dip"
                    android:layout_marginLeft="2dip"
                    android:layout_marginStart="2dip"
                    android:id="@+id/shopLayoutEquippable"
                    >
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="SOLD OUT"
                        android:textSize="16sp"
                        android:textStyle="bold"
                        android:textColor="#FFFFFF"
                        android:gravity="center"
                        android:layout_centerInParent="true"
                        android:id="@+id/shopWarningEquippable"
                        />
                    <RelativeLayout
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:clickable="true"
                        android:onClick="equippableClick"
                        android:id="@+id/shopLayoutEquippableInner"
                        >
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="20G"
                            android:textColor="#FFFFFF"
                            android:layout_centerHorizontal="true"
                            android:layout_marginTop="10dip"
                            android:clickable="true"
                            android:onClick="equippableClick"
                            android:id="@+id/shopPriceEquippable"
                            />
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="20dip"
                            android:text="123456789012"
                            android:textColor="#FFFFFF"
                            android:textSize="14sp"
                            android:textStyle="bold"
                            android:maxLength="12"
                            android:maxLines="1"
                            android:typeface="monospace"
                            android:gravity="center"
                            android:layout_centerHorizontal="true"
                            android:layout_below="@id/shopPriceEquippable"
                            android:layout_marginTop="2dip"
                            android:clickable="true"
                            android:onClick="equippableClick"
                            android:id="@+id/shopNameEquippable"
                            />
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="12345678901234567890121234567890123456789012"
                            android:textColor="#FFFFFF"
                            android:textSize="12sp"
                            android:typeface="monospace"
                            android:gravity="center"
                            android:layout_centerHorizontal="true"
                            android:layout_below="@+id/shopNameEquippable"
                            android:layout_marginTop="2dip"
                            android:clickable="true"
                            android:onClick="equippableClick"
                            android:id="@+id/shopInfoEquippable"
                            />

                    </RelativeLayout>
                </RelativeLayout>
                <RelativeLayout
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:background="#AAAAAA"
                    android:layout_weight="1"
                    android:layout_marginRight="2dip"
                    android:layout_marginEnd="2dip"
                    android:layout_marginLeft="2dip"
                    android:layout_marginStart="2dip"
                    android:id="@+id/shopLayoutSpell"
                    >
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="SOLD OUT"
                        android:textSize="16sp"
                        android:textStyle="bold"
                        android:textColor="#FFFFFF"
                        android:gravity="center"
                        android:layout_centerInParent="true"
                        android:id="@+id/shopWarningSpell"
                        />
                    <RelativeLayout
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:clickable="true"
                        android:onClick="spellClick"
                        android:id="@+id/shopLayoutSpellInner"
                        >
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="20G"
                            android:textColor="#FFFFFF"
                            android:layout_centerHorizontal="true"
                            android:layout_marginTop="10dip"
                            android:clickable="true"
                            android:onClick="spellClick"
                            android:id="@+id/shopPriceSpell"
                            />
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="20dip"
                            android:text="123456789012"
                            android:textColor="#FFFFFF"
                            android:textSize="14sp"
                            android:textStyle="bold"
                            android:maxLength="12"
                            android:maxLines="1"
                            android:typeface="monospace"
                            android:gravity="center"
                            android:layout_centerHorizontal="true"
                            android:layout_below="@id/shopPriceSpell"
                            android:layout_marginTop="2dip"
                            android:clickable="true"
                            android:onClick="spellClick"
                            android:id="@+id/shopNameSpell"
                            />
                        <TextView
